<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>jspProject</display-name>
    <session-config>
    <session-timeout>15</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>media</servlet-name>
    <servlet-class>media.MediaController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>media</servlet-name>
    <url-pattern>/media_servlet/*</url-pattern>
  </servlet-mapping>
  
 <servlet>
    <servlet-name>drama</servlet-name>
    <servlet-class>drama.DramaController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>drama</servlet-name>
    <url-pattern>/drama_servlet/*</url-pattern>
  </servlet-mapping>
  
 <servlet>
    <servlet-name>enter</servlet-name>
    <servlet-class>enter.EnterController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>enter</servlet-name>
    <url-pattern>/enter_servlet/*</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>culture</servlet-name>
    <servlet-class>culture.CultureController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>culture</servlet-name>
    <url-pattern>/culture_servlet/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>log</servlet-name>
    <servlet-class>log.LogController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>log</servlet-name>
    <url-pattern>/log_servlet/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>notiec</servlet-name>
    <servlet-class>notiec.notiecController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>notiec</servlet-name>
    <url-pattern>/notiec_servlet/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>guest</servlet-name>
    <servlet-class>guest.GuestController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>guest</servlet-name>
    <url-pattern>/guest_servlet/*</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>